 MACRO
&NAME  PLIST &P1
 LCLA &NPLIST,&CPLIST
 AIF ('&P1' EQ '').E2
 AIF ('&P1'(1,1) NE '(').E1
&NPLIST SETA N'&P1
&CPLIST SETA 1
&NAME  DS    0F
.B1 AIF ('&P1(&CPLIST)' EQ '').B2
  DC    A(&P1(&CPLIST))
 AGO .B3
.B2  DC    A(0)
.B3 ANOP
&CPLIST SETA 1+&CPLIST
 AIF (&CPLIST GT &NPLIST).B4
 AGO .B1
.B4  ORG   *-4
  DC    X'80'
  ORG   *+3
 MEXIT
.E1 MNOTE 2,'OPND NOT ENCLOSED IN PARENTHESES. STMT IGNORED.'
 MEXIT
.E2 MNOTE 2,'NO OPND. STATEMEMT IGNORED.'
 MEXIT
 MEND
